{
  "name": "yjs-quill-demo",
  "version": "1.0.0",
  "description": "Yjs ‚ù§ Quill",
  "author": "Kevin Jahns <kevin.jahns@pm.me>",
  "license": "UNLICENSE",
  "scripts": {
    "dist": "npm run build:css && webpack --mode=production",
    "start": "webpack-dev-server --open-page quill.html",
    "dev": "npm-run-all --parallel dev:*",
    "dev:webpack": " webpack serve --port 3000 --config ./webpack.config.js  --mode development",
    "alt": " webpack serve --port 4200 --config ./webpack.config.js  --mode development",
    "build:css": " postcss src/assets/tailwind.css -o src/assets/main.css --minify",
    "dev:css": "TAILWIND_MODE=watch postcss src/assets/tailwind.css -o src/assets/main.css --watch"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "quill": "^1.3.7",
    "quill-cursors": "^2.2.1",
    "react": "^16.8.0",
    "react-dom": "^17.0.2",
    "react-quill": "^1.3.5",
    "react-router-dom": "^6.2.2",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.3.0",
    "y-leveldb": "^0.1.1",
    "y-quill": "0.1.4",
    "y-websocket": "^1.3.15",
    "yjs": "^13.5.8"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^6.2.1",
    "tailwindcss": "^3.0.23",
    "webpack-dev-server": "^4.7.4"
  }
}
